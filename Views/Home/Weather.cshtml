@model WeatherApp.Models.ApiClient


<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>


<ul>
	<li><b>Location:</b> @Model.data.name</li>

	<li><b>Temperature:</b> @Model.data.main.temp F degrees.</li>
	<li><b>Condition:</b> @Model.data.weather[0].main</li>

	<li><b>Humidity:</b> @Model.data.main.humidity%</li>
	<li><b>Wind:</b> @Model.data.wind.speed mph </li>


</ul>
	


