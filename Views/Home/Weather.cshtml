@using WeatherApp.Models


<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>


	
<ul>
	<li><b>Location:</b> @Model.Response.name</li>

	<li><b>Temperature:</b> @Model.Response.main.temp F degrees.</li>
	<li><b>Condition:</b> @Model.Response.weather[0].main</li>

	<li><b>Humidity:</b> @Model.Response.main.humidity%</li>
	<li><b>Wind:</b> @Model.Response.wind.speed mph </li>


</ul>

